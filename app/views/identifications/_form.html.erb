<%= form_with(model: [@user, @identification], url: user_identifications_path, local: true) do |form| %>
  <% if identification.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(identification.errors.count, "error") %> prohibited this identification from being saved:</h2>

      <ul>
      <% identification.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="field">
    <%= form.label :number %>
    <%= form.text_field :number %>
  </div>

  <div class="field">
    <%= form.label :state %>
    <%= form.text_field :state %>
  </div>

  <div class="field">
    <%= form.label :expiration %>
    <%= form.date_select :expiration %>
  </div>

  <div class="field">
    <%= form.file_field :image_upload %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
